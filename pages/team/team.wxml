<!--游客显示按钮-->
<view class="{{userType == 1?'hide':'show'}}" style='background-color: rgb(242, 242, 242);height: 250px;'>
  <view  style="padding:30rpx 60rpx;display:flex;align-items: center;justify-content:space-between;">
    <button class = 'btn' bindtap='signTeam'> 签到 </button>
    <button class = 'btn' bindtap='assemble'> 集合 </button>
    <button class = 'btn' bindtap='queryRoom'> 房间 </button>
    <button class = 'btn' bindtap='allNotice'> 公告 </button>
  </view>
  <view  style="padding:30rpx 60rpx;display:flex;align-items: center;justify-content:space-between;">
    <button class = 'btn' bindtap='oldTeam'> 足迹 </button>
    <button class = 'btn'bindtap='strategy'> 攻略 </button>
    <button class = 'btn' bindtap='shop'> 特产 </button>
    <button class = 'btn' bindtap='more'> 更多 </button>
  </view>
</view>








