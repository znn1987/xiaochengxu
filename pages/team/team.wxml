 <!--公告-->
<view class="{{ noticeCode == 0?'hide':'show'}}" style="padding:30rpx;background-color: rgb(1, 247, 54);color: rgb(247, 1, 1);">
<view  style="display:flex;justify-content:space-between;">
      <view >
        <text bindtap='newNotice' data-text='{{noticeCode}}'>团队公告：{{title}}</text>
      </view>
      <view >
        <text bindtap='allNotice'>更多……</text>
      </view>
  </view>
</view>

<view style="background-image: url('data:image/jpeg;base64,');background-size:100% 100%;-moz-background-size:100% 100%;">
<view style='display: flex;justify-content:space-between;padding:30rpx;'>
<text>{{address}}</text>
<text>{{time}}</text>
</view>
<view style='padding:10rpx 30rpx'>天气：{{weather.weather.data}}</view>
<view style='padding:10rpx 30rpx'>温度：{{weather.temperature.data}}°</view>
<view style='padding:10rpx 30rpx'>湿度：{{weather.humidity.data}}</view>
<view style='padding:10rpx 30rpx'>风向：{{weather.winddirection.data}}</view>
<view style='padding:10rpx 30rpx'>风力：{{weather.windpower.data}}</view>
</view>


  <!--队伍信息-->
<view style="padding:30rpx;">
    <view  style="display:flex;padding:30rpx 60rpx;align-items: center;justify-content:space-between;">
<!--
<image style="height: 70rpx;width: 70rpx;" src ='../images/front1.jpg'></image>
-->
        <view>
        <text class = " {{code == 1 ?'nodownbtn':'downbtn'}}"bindtap='addTeam'>  {{teamName}} </text> 
        </view>
        <view >
        <text class = " {{code == 1 ?'nodownbtn':'downbtn'}}" bindtap='addTeam'> {{code == 1?'':allUser}}{{code == 1 ?'':'人'}} </text> 

        <image class="{{userType != 1?'hide':'show'}}" style="float:right;height: 50rpx;width: 50rpx;" bindtap='addTeam' src ='../images/right.png'></image>

        </view>
     </view>
    <view class="{{code == 1?'hide':'show'}}" style="display:flex;align-items: center;justify-content: center;">
    <text  bindtap='addTeam' >  {{code == 1?'':start}} {{code == 1?'':'-'}} {{code == 1?'':end}} </text> 
    </view>
</view>


