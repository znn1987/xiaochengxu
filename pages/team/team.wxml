<!--导游显示按钮-->
<view class="{{userType == 1?'show':'hide'}}" style='background-color: rgb(242, 242, 242);height: 250px;'>
<view  style="padding:30rpx 60rpx;display:flex;align-items: center;justify-content:space-between;">
  <button class = 'btn' bindtap='userBtn'> 队员 </button>
  <button class = 'btn' bindtap='room'> 分房 </button>
  <button class = 'btn' bindtap='queryRoom'> 查房 </button>
  <button class = 'btn' bindtap='assemble'> 集合 </button>
</view>
<view  style="padding:30rpx 60rpx;display:flex;align-items: center;justify-content:space-between;">
  <button class = 'btn' bindtap='signTeam'> 签到 </button>
  <button class = 'btn' bindtap='noticeSet'> 公告 </button>
  <button class = 'btn' bindtap='oldTeam'> 成就 </button>
  <button class = 'btn' bindtap='more'> …… </button>
</view>
</view>

