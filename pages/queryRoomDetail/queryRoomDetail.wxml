<!--pages/queryRoomDetail/queryRoomDetail.wxml-->


  <block wx:for="{{users}}" wx:key="idx" wx:for-item="user">
    <view class='waibu'>
<!--     第一行入住日期      -->
      <view class='hang'>
        <view class='datelable'>
          入住日期:
        </view>
        <view class='dateTime'>
          {{user.inDate}}--{{user.outDate}}
        </view>
      </view>
      <!--     第二行      -->
      <view class='hang'>
        <view class='roomNumber'>房间号：{{user.roomNum}}</view>
        <view class='roomType'>房间类型：{{user.typeName}}</view>
      </view>
      <!--     第三行      -->
      <view class='hang'>
        <view class='tour'>导游：{{user.tourName}}</view>
      </view>
       <!--     第五行      -->
      <view class='hang'>
        <view class='userName'>入住人：{{user.userName}}</view>
        <view class='sex'>性别：{{user.userSex}}</view>
      </view>
       <!--     第六行      -->
      <view class='hang'>
        <view class='cord'>身份证号：{{user.userCode}}</view>
      </view>
      <view class='geceng'></view>
    </view> 
